set mouse=a
set number
set shiftwidth=4
set tabstop=2
set expandtab
set autoindent
syntax on
colorscheme elflord
filetype on
filetype plugin on
filetype indent on

nmap <silent> <C-Up> :wincmd k<CR>
nmap <silent> <C-Down> :wincmd j<CR>
nmap <silent> <C-Left> :wincmd h<CR>
nmap <silent> <C-Right> :wincmd l<CR>
